{
  "name": "react-has-mounted",
  "version": "0.1.0",
  "description": "High Order Component for React SSR",
  "main": "index.js",
  "author": "Luchanskiy Oleg <luchansodev@gmail.com>",
  "repository": "https://github.com/Luchanso/react-has-mounted",
